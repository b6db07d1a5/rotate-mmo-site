# Yarn configuration file
# Enable yarn v2+ features
nodeLinker: node-modules

# Disable telemetry
telemetry: false

# Set cache folder
cacheFolder: .yarn/cache

# Enable strict SSL
strict-ssl: true

# Set registry
registry: https://registry.yarnpkg.com

# Enable immutable installs
enableImmutableInstalls: true

# Set network timeout
networkTimeout: 300000
